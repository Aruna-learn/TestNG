<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoSuite">
	<test name="OrangeHRMLogin">
		<classes>
			<class name="com.testngpractise.OrangeHRM">
				<methods>
					<include name="LaunchApp" />
					<include name="EnterLoginDetails" />
					<include name="VerifyLogin" />
				</methods>
			</class>
		</classes>
	</test>


	<test name="OrangeHRMMyInfo">
		<classes>
			<class name="com.testngpractise.OrangeHRM">
				<methods>
					<include name="LaunchApp" />
					<include name="EnterLoginDetails" />
					<include name="MyInfo" />
					<include name="VerifyMyinfo" />
				</methods>
			</class>
		</classes>
	</test>

	<test name="TestGoogle">
		<classes>
			<class name="com.testngpractise.FirstTestNG">
				<methods>
					<include name="google" />
					<include name="Facebook" />
				</methods>
			</class>
		</classes>
	</test>



</suite> <!-- Suite -->
